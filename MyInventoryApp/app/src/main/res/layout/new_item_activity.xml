<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/new_item_ac"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFDF3"
    android:clipToOutline="true">


    <View
        android:id="@+id/image_2"
        android:layout_width="match_parent"
        android:layout_height="95dp"
        android:background="@drawable/myinventorysmall"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/serial_numb"
        android:layout_width="298dp"
        android:layout_height="43dp"
        android:layout_marginTop="96dp"
        android:gravity="center_vertical"
        android:text="@string/serial_numb"
        android:textAppearance="@style/serial_numb"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/image_2" />

    <View
        android:id="@+id/line_2"
        android:layout_width="298dp"
        android:layout_height="1dp"
        android:background="@drawable/line_2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/serial_numb" />

    <TextView
        android:id="@+id/acquired_da"
        android:layout_width="298dp"
        android:layout_height="43dp"
        android:layout_marginTop="7dp"
        android:gravity="center_vertical"
        android:text="@string/acquired_da"
        android:textAppearance="@style/acquired_da"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/line_2" />

    <View
        android:id="@+id/line_5"
        android:layout_width="298dp"
        android:layout_height="1dp"
        android:background="@drawable/line_2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/acquired_da" />

    <TextView
        android:id="@+id/make"
        android:layout_width="298dp"
        android:layout_height="43dp"
        android:layout_marginTop="7dp"
        android:gravity="center_vertical"
        android:text="@string/make"
        android:textAppearance="@style/make"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/line_5" />

    <View
        android:id="@+id/line_4"
        android:layout_width="298dp"
        android:layout_height="1dp"
        android:background="@drawable/line_2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/make" />

    <TextView
        android:id="@+id/estimated_p"
        android:layout_width="298dp"
        android:layout_height="43dp"
        android:layout_marginTop="7dp"
        android:gravity="center_vertical"
        android:text="@string/estimated_p"
        android:textAppearance="@style/estimated_p"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/line_4" />

    <View
        android:id="@+id/line_3"
        android:layout_width="298dp"
        android:layout_height="1dp"
        android:background="@drawable/line_2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/estimated_p" />

    <TextView
        android:id="@+id/description"
        android:layout_width="298dp"
        android:layout_height="43dp"
        android:layout_marginTop="7dp"
        android:gravity="center_vertical"
        android:text="@string/description"
        android:textAppearance="@style/description"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/line_3" />

    <View
        android:id="@+id/line_6"
        android:layout_width="298dp"
        android:layout_height="1dp"
        android:background="@drawable/line_2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/description" />

    <Button
        android:id="@+id/rectangle_1"
        android:layout_width="119dp"
        android:layout_height="48dp"
        android:layout_marginTop="52dp"
        android:backgroundTint="@color/orange"
        android:text="@string/scan_image"
        android:textAppearance="@style/scan_image"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/description" />

    <ImageView
        android:id="@+id/icon_chevro"
        android:layout_width="64dp"
        android:layout_height="86dp"
        android:layout_marginTop="45dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rectangle_1"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/baseline_arrow_forward_ios_24" />


</androidx.constraintlayout.widget.ConstraintLayout>